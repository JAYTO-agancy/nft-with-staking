model GeneratedNft {
  id String @id @default(uuid())
  // <-- Main Fields -->

  edition     Int    @unique
  name        String
  rarityLevel Int
  rarityName  String
  dna         String @unique
  imageUrl    String
  jsonUrl     String // Добавляем поле для URL JSON файла
  metadata    Json

  // <--/ Main Fields /-->
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@map("generated_nft")
}
